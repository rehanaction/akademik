CREATE TABLE [sdm].[ms_pegawai] (
[idpegawai] int NOT NULL IDENTITY(1,1) ,
[nip] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[namadepan] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[namatengah] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[namabelakang] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[gelardepan] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[gelarbelakang] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[jeniskelamin] char(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tgllahir] datetime NULL ,
[tmplahir] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[statusnikah] varchar(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idtipepeg] varchar(3) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL ,
[idjenispegawai] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL ,
[idhubkerja] varchar(3) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idstatusaktif] varchar(2) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idjfungsional] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idpangkat] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idjstruktural] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodejabatanatasan] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idpendidikan] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodept] varchar(6) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodefakultas] varchar(3) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodejurusan] varchar(7) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodebidang] varchar(10) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[npwp] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nodosen] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nidn] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nippns] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[jmlanak] numeric(2) NULL ,
[kelompokdosen] varchar(10) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[jamkerja] numeric(3) NULL ,
[bebanskswajib] numeric(3) NULL ,
[idagama] varchar(2) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idkewarganegaraan] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idbank] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[norekening] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[cabangbank] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[noskcalon] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglcalon] datetime NULL ,
[noskpengangkatan] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglpengangkatan] datetime NULL ,
[noskmasuk] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglmasuk] datetime NULL ,
[masakerjathngol] numeric(2) NULL ,
[masakerjablngol] numeric(2) NULL ,
[tmasakerjanosk] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tmttglsk] datetime NULL ,
[tglwafat] datetime NULL ,
[tglpensiun] datetime NULL ,
[tglterusan] datetime NULL ,
[issertifikasi] char(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nosertifikasi] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglsertifikasi] datetime NULL ,
[noaskes] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kategoriaskes] varchar(10) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[statusaskes] char(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglaskes] datetime NULL ,
[nodapen] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[statusdapen] char(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tgldapen] datetime NULL ,
[alamatktp] varchar(150) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idkelurahanktp] varchar(8) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodeposktp] varchar(6) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[noktp] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tglktp] datetime NULL ,
[tglktphabis] datetime NULL ,
[alamat] varchar(150) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idkelurahan] varchar(8) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[kodepos] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[telepon] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nohp] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[teleponkantor] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[email] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[sukubangsa] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[goldarah] varchar(3) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[foto] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[tinggi] numeric(4) NULL ,
[beratbadan] numeric(4) NULL ,
[ukuranbaju] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[ukurankacamata] varchar(50) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[jarakrumah] numeric(6) NULL ,
[keterangan] varchar(255) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[t_username] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[t_ipaddress] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[t_updatetime] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idunit] int NULL ,
[emailatasan] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idmilikpeg] varchar(3) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idbankhonor] varchar(5) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[norekeninghonor] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[cabangbankhonor] varchar(30) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[emailpribadi] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[anrekening] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[anrekeninghonor] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[nohandkey] varchar(10) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idunitbase] int NULL ,
[tmtpangkat] date NULL ,
[nik] varchar(25) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[niphkey] varchar(10) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[idpegawaiatasan] int NULL ,
[kodekeldosen] varchar(8) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[usernameldap] varchar(100) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[mkmengajar] varchar(4) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
[ispasangankerja] char(1) COLLATE SQL_Latin1_General_CP1_CI_AS NULL ,
CONSTRAINT [pk_ms_pegawai] PRIMARY KEY NONCLUSTERED ([idpegawai]),
CONSTRAINT [fk_bank] FOREIGN KEY ([idbank]) REFERENCES [sdm].[ms_bank] ([idbank]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_fakultas] FOREIGN KEY ([kodefakultas]) REFERENCES [sdm].[ms_fakultas] ([kodefakultas]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_hubkerja] FOREIGN KEY ([idhubkerja]) REFERENCES [sdm].[ms_hubkerja] ([idhubkerja]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_idbankhonor] FOREIGN KEY ([idbankhonor]) REFERENCES [sdm].[ms_bank] ([idbank]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_jabfungsional] FOREIGN KEY ([idjfungsional]) REFERENCES [sdm].[ms_fungsional] ([idjfungsional]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_jurusan] FOREIGN KEY ([kodejurusan]) REFERENCES [sdm].[ms_jurusan] ([kodejurusan]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_kelompokdosen] FOREIGN KEY ([kodekeldosen]) REFERENCES [sdm].[ms_kelompokdosen] ([kodekeldosen]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_kelurahan] FOREIGN KEY ([idkelurahan]) REFERENCES [sdm].[lv_kelurahan] ([idkelurahan]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_kelurahanktp] FOREIGN KEY ([idkelurahanktp]) REFERENCES [sdm].[lv_kelurahan] ([idkelurahan]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_kodebidang] FOREIGN KEY ([kodebidang]) REFERENCES [sdm].[ms_bidang] ([kodebidang]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_milikpeg] FOREIGN KEY ([idmilikpeg]) REFERENCES [sdm].[ms_kepemilikanpegawai] ([idmilikpeg]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_pangkat] FOREIGN KEY ([idpangkat]) REFERENCES [sdm].[ms_pangkat] ([idpangkat]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_pendidikanpeg] FOREIGN KEY ([idpendidikan]) REFERENCES [sdm].[lv_jenjangpendidikan] ([idpendidikan]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_pt] FOREIGN KEY ([kodept]) REFERENCES [sdm].[ms_pt] ([kodept]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_statusaktif] FOREIGN KEY ([idstatusaktif]) REFERENCES [sdm].[lv_statusaktif] ([idstatusaktif]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [fk_struktural] FOREIGN KEY ([idjstruktural]) REFERENCES [sdm].[ms_struktural] ([idjstruktural]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_unithomebasepegawai] FOREIGN KEY ([idunit]) REFERENCES [sdm].[ms_unit] ([idunit]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_unitpeg] FOREIGN KEY ([idunit]) REFERENCES [sdm].[ms_unit] ([idunit]) ON DELETE NO ACTION ON UPDATE NO ACTION,
CONSTRAINT [fk_warganegara] FOREIGN KEY ([idkewarganegaraan]) REFERENCES [sdm].[ms_warganegara] ([idkewarganegaraan]) ON DELETE NO ACTION ON UPDATE CASCADE,
CONSTRAINT [ckc_jeniskelamin_ms_pegaw] CHECK (([jeniskelamin] IS NULL OR ([jeniskelamin]='P' OR [jeniskelamin]='L'))),
CONSTRAINT [ckc_statusnikah_ms_pegaw] CHECK (([statusnikah] IS NULL OR ([statusnikah]='J' OR [statusnikah]='D' OR [statusnikah]='N' OR [statusnikah]='S')))
)